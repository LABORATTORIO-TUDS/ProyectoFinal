/pom.xml.tag
/pom.xml.releaseBackup
/pom.xml.versionsBackup
/release.properties
dependency-reduced-pom.xml
buildNumber.properties
.target/
.mvn/wrapper/maven-wrapper.jar
/target/

# Gradle
.gradle/
/build/
/gradle-app.setting
!gradle/wrapper/gradle-wrapper.jar
gradle-wrapper.properties

# IDEs
/.idea/
/*.iml
/*.ipr
/*.iws
.classpath
.project
.settings/

# JetBrains Rider/Other
.idea/

# Sistema operativo
.DS_Store
Thumbs.db
desktop.ini
ehthumbs.db

# Editores / temporales
*~ 
*.swp
*.swo
*.bak
*.tmp

